{
    "name": "eden-creative/craft-flickr-gallery",
    "description": "Import photos directly from your Flickr account",
    "type": "craft-plugin",
    "license": "mit",
    "version": "1.0.3",
    "keywords": [
      "craft",
      "cms",
      "craftcms",
      "craft-plugin",
      "flickr",
      "image",
      "assets",
      "gallery",
      "import"
    ],
    "support": {
        "email": "developers@edencreative.co",
        "issues": "https://github.com/edencreativeco/craft-flickr-gallery/issues?state=open",
        "source": "https://github.com/edencreativeco/craft-flickr-gallery",
        "docs": "https://github.com/edencreativeco/craft-flickr-gallery",
        "rss": "https://github.com/edencreativeco/craft-flickr-gallery/releases.atom"
    },
    "authors": [
        {
            "name": "Eden Creative",
            "homepage": "https://edencreative.co"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.0",
        "league/oauth1-client": "^1.10"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "edencreative\\craftflickrgallery\\": "src/"
        }
    },
    "extra": {
        "handle": "craft-flickr-gallery",
        "name": "Flickr Gallery",
        "developer": "Eden Creative",
        "documentationUrl": "https://github.com/edencreativeco/craft-flickr-gallery"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
